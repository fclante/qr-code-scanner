<%- include('../layouts/layout',{ title: 'Medarbejder Varer', body: `
<h1>Medarbejder - Varer</h1>
<p>Content for Varer</p>
<table border="1">
  <thead>
    <tr>
      <th>ID</th>
      <th>Price</th>
      <th>Date Added</th>
      <th>Expiration Date</th>
      <th>Quantity</th>
      <th>In Stock</th>
      <th>Actions</th>
    </tr>
  </thead>
  <tbody>
    <% data.forEach((row) => { %>
      <tr>
        <td><%= row.id %></td>
        <td>$<%= row.price.toFixed(2) %></td>
        <td><%= row.date_added %></td>
        <td><%= row.expiration_date %></td>
        <td><%= row.quantity %></td>
        <td><%= row.in_stock ? 'Yes' : 'No' %></td>
        <td>
          <a href="/generate/<%= row.id %>">Print QR</a>
        </td>
      </tr>
    <% }); %>
  </tbody>
</table>

` }) %>

